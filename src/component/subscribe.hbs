<section class="py-5">
    {{#with subscribe}}
        <div class="row">
            <div class="col">
                <div class="card">
                    <div class="card-body text-bg-dark d-flex justify-content-between">
                        <div>
                            <h2>{{header}}</h2>
                            <p>{{description}}</p>
                        </div>
                        <div class=" d-flex align-items-center">
                            {{#each buttons}}
                                <span class="mx-1">{{> btnPrimary}}</span>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {{/with}}
</section>