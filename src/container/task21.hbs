<header>
  <section>
    <div class='card'>
      <div class='card-body'>
        {{#with heading}}
          {{#with main}}
            <div class='card mb-4'>
              <div class='card-body'>
                <h1>{{title}}</h1>
                <p>{{text}}</p>
                {{#with button_text}}
                  <div class='my-2'>
                    <button
                      class='btn btn-primary'
                    >{{solid}}</button>
                  </div>
                  <div class='my-2'>
                    <button
                      class='btn btn-outline-primary'
                    >{{outline}}</button>
                  </div>
                {{/with}}
              </div>
            </div>
          {{/with}}

          {{#with second}}
            <div class='card mb-4'>
              <div class='card-body'>
                <h2>{{title}}</h2>
                <p>{{text}}</p>
                {{#with button_text}}
                  <div class='my-2'>
                    <button
                      class='btn btn-secondary'
                    >{{solid}}</button>
                  </div>

                  <div class='my-2'>
                    <button
                      class='btn btn-outline-secondary'
                    >{{outline}}</button>
                  </div>
                {{/with}}
              </div>
            </div>
          {{/with}}

          {{#with subblock}}
            <div class='card'>
              <div class='card-body'>
                <h3>{{title}}</h3>
                <p>{{text}}</p>
                {{#with button_text}}
                  <div class='my-2'>
                    <button
                      class='btn btn-dark'
                    >{{solid}}</button>
                  </div>

                  <div class='my-2'>
                    <button
                      class='btn btn-outline-dark'
                    >{{outline}}</button>
                  </div>
                {{/with}}
              </div>
            </div>
          {{/with}}
        {{/with}}
      </div>
    </div>
  </section>
</header>
<main>
  <section>
    <div class='card my-4'>
      <div class='card-body'>
        {{#with nested_display}}
          <div class='display-1'>{{title}}</div>
          <div class='card'>
            <div class='card-body'>
              {{#with children_display}}
                <div class='display-4'>{{title}}</div>

                <p>{{text}}</p>
                {{#with button}}
                  <div class='my-2'>
                    <button
                      class='btn btn-outline-warning'
                    >{{text}}</button>
                  </div>

                {{/with}}
              {{/with}}
            </div>
          </div>

          <h6 class='mt-2'>{{header_text}}</h6>
          <div class='my-2'>
            <button
              class='btn btn-info'
            >{{button.text}}</button>
          </div>
        {{/with}}
      </div>
    </div>
  </section>

  <section>
    <div class='card'>
      <div class='card-body'>
        {{#with paragraph}}
          <p class='text-uppercase'>{{title}}</p>
          <p class='text-lowercase'>{{text}}</p>
          {{#with button}}
            <a
              href='{{url}}'
              class='btn btn-link'
            >{{text}}</a>

          {{/with}}
          <div class='card mt-3'>
            <div class='card-body'>
              {{#each list_name}}
                <ul>
                  <li>
                    <a
                      href='{{url}}'
                      class='btn btn-link'
                    >{{text}}</a>
                  </li>
                </ul>
              {{/each}}
            </div>
          </div>
        {{/with}}
      </div>
    </div>
  </section>
</main>