<main>
    {{#with web}}
        {{#each languages}}
            <section>
                {{> title data=name big=true}}
                    <div>
                        <em>version: {{version}}</em>
                    </div>
                    <div class="mt-3">
                        {{description}}
                    </div>
                    {{#if elements}}
                        <ul>
                            {{> title data='Elements:'}}
                            
                            {{#each elements}}
                                <li>
                                    {{> itemlist}}
                                    <ul>
                                        <div class="mt-3">attributes:</div>
                                        {{#each attributes}}
                                            <li>
                                                <strong>{{name}}</strong>
                                                <p>{{description}}</p>
                                            </li>
                                        {{/each}}
                                    </ul>
                                </li>
                            {{/each}}
                        </ul>
                    {{/if}}
                    {{#if properties}}
                        <ul>
                            {{> title data='Properties:'}}
                            {{#each properties}}
                                <li>
                                    {{> itemlist}}
                                    <ul>
                                        <div class="mt-3">values:</div>
                                        {{#each values}}
                                            <li>
                                                <strong>{{value}}</strong>
                                                <p>{{description}}</p>
                                            </li>
                                        {{/each}}
                                    </ul>
                                </li>
                            {{/each}}
                        </ul>
                    {{/if}}
                    {{#if functions}}
                        <ul>
                            {{> title data='Functions:'}}
                            {{#each functions}}
                                <li>
                                    {{> itemlist}}
                                    <ul>
                                        <div class="mt-3">parameters:</div>
                                        {{#each parameters}}
                                            <li>
                                                <strong>{{name}}</strong>
                                                <p>type: {{type}}</p>
                                                <p>{{description}}</p>
                                            </li>
                                        {{/each}}
                                    </ul>
                                </li>
                            {{/each}}
                        </ul>
                    {{/if}}
            </section>
        {{/each}}
    {{/with}}
</main>